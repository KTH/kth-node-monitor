{
  "name": "@kth/monitor",
  "version": "4.0.0-alpha.0",
  "description": "Helper for monitor page checks.",
  "automaticPublish": "true",
  "main": "dist/monitorUtil.js",
  "author": {
    "name": "KTH",
    "email": "infosys@kth.se",
    "url": "https://github.com/KTH"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/kth-node-monitor"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch src",
    "test:notify": "jest --watch --notify",
    "test:coverage": "rm -rf ./coverage && jest --collectCoverage --detectOpenHandles",
    "build": "./build.sh",
    "build:watch": "tsc --watch",
    "lint": "eslint \"**/*.js\" --quiet",
    "lint-v": "eslint \"**/*.js\"",
    "prepare": "if [ -f ./node_modules/.bin/husky ]; then husky install; fi"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^3.0.10",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.15.11",
    "eslint": "^8.23.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "@types/express": "^4.17.17"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "dependencies": {
    "@kth/in-memory-cache": "^0.1.51",
    "@kth/log": "^4.0.5"
  },
  "prettier": {
    "//": "configuration version 2020-04-10 - might also reside in .prettierrc.json",
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
